{
  "version": "1.0.0-*",
  "authors": [ "Marcin Smółka" ],
  "testRunner": "mstest",

  "buildOptions": {
    "warningsAsErrors": false,
    "xmlDoc": true,
    "additionalArguments": [ "/ruleset:../../CommonRuleSet.ruleset", "/additionalfile:../../stylecop.json" ]
  },

  "dependencies": {
    "ZNetCS.AspNetCore.IpFiltering": {
      "version": "*",
      "target": "project"
    },
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "dotnet-test-mstest": "1.1.2-preview",
    "Microsoft.AspNetCore.TestHost": "1.1.0",
    "Microsoft.Net.Http.Headers": "1.1.0",
    "MSTest.TestFramework": "1.1.11",
    "Microsoft.Extensions.Configuration.Json": "1.1.0"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],

      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0",
          "type": "platform"
        }
      }
    }
  }
}
